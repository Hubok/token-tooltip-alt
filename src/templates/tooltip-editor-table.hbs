{{#each settings}}
    <h2>{{disposition}}</h2>

    <div class="form-group">
        <table class="{{../moduleName}}-table_{{../type}}_{{disposition}}" disposition="{{disposition}}">
            <thead>
            <tr>
                <th scope="col" class="{{../moduleName}}-col_color">Color</th>
                <th scope="col" class="{{../moduleName}}-col_icon">Icon</th>
                <th scope="col" class="{{../moduleName}}-col_value">Value</th>
                <th scope="col" class="{{../moduleName}}-col_expression">Exp</th>
                <th scope="col" class="{{../moduleName}}-col_isNumber">Nr</th>
                <th scope="col" class="{{../moduleName}}-col_delete">Delete</th>
            </tr>
            </thead>

            <tbody class="{{../moduleName}}-tbody_{{../type}}_{{disposition}}">
            {{#each items}}
                {{> 'modules/token-tooltip-alt/templates/tooltip-editor-table-row.hbs' index=@index moduleName=../../moduleName disposition=../disposition type=../type item=this}}
            {{/each}}
            </tbody>
        </table>
    </div>
    <div class="form-group">
        <button type="button" disposition="{{disposition}}" name="{{../type}}.items.{{disposition}}.add" class="{{../moduleName}}-button add_{{disposition}}">
            <i class="fa fa-plus"></i> <span>Add</span>
        </button>
        {{#if (ne @index 0)}}
            <button type="button" disposition="{{disposition}}" name="{{../type}}.items.{{disposition}}.clone" class="{{../moduleName}}-footer_button clone_{{disposition}}">
                <i class="fas fa-edit"></i> <span>Copy from above</span>
            </button>
        {{/if}}
    </div>
{{/each}}